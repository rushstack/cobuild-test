{
  "$schema": "https://developer.microsoft.com/json-schemas/tsdoc/v0/tsdoc.schema.json",

  "extends": ["@microsoft/api-extractor/extends/tsdoc-base.json"],

  "tagDefinitions": [
    {
      "tagName": "@docCategory",
      "syntaxKind": "inline"
    },
    {
      "tagName": "@myCustomTag",
      "syntaxKind": "modifier"
    }
  ],
  "supportForTags": {
    "@myCustomTag": true,
    "@docCategory": true
  }
}
